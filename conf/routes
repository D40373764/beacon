# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
### NoDocs ###
GET     /                           controllers.HomeController.index

### NoDocs ###
GET 	/attendees/:eventID			controllers.AttendanceController.getAttendeesByEvent(eventID: String)
POST 	/attendee					controllers.AttendanceController.postAttendee
GET 	/questions/:eventID			controllers.AttendanceController.getQuestionsByEvent(eventID: String)
POST 	/feedback					controllers.AttendanceController.postFeedback


# Map static resources from the /public folder to the /assets URL path
### NoDocs ###
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

### NoDocs ###
GET   	/docs/swagger-ui/*file      controllers.Assets.at(path:String="/public/lib/swagger-ui", file:String)

